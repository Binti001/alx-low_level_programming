#include "main.h"

/**
 * _strcat - concatenates two strings
 * @dest: first parameter
 * @src: second parameter
 * Returns: pointer to a string
 */

char *_strcat(char *dest, char *src)
{
	int len1;
	int len2;
	char s1[];
	char s2[];
	int i;

	for (i = 0; i <= len2; i++)
	{
		s1[len1 + i] = s2[i];
	}
	return (s1);
